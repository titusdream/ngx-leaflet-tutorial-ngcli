{
  "maps": [
    {
      "urlTemplate": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png",
      "options": {
        "maxZoom": 15,
        "subdomains": [ "a", "b", "c" ],
        "attribution": "Open Street Map"
      }
    },
    {
      "urlTemplate": "http://{s}.google.com/vt/lyrs=m&x={x}&y={y}&z={z}",
      "options": {
        "maxZoom": 20,
        "subdomains": [ "mt0", "mt1", "mt2", "mt3" ]
      }
    }
  ],
  "mapId": 0,
  "zoomPanOptions": {
    "animate": true,
    "duration": 1
  },
  "record": {
    "zoom": 5,
    "center": [25, 3]
  },
  "viewOffset": [-450, 200],
  "template": "<div class='img-ctn img-ctn-{dir}' style='background-image: url(\"{image}\")'><img class='img-frame' src='../assets/photos/frame-{dir}.png'/><div class='caption'>07-2017 Montagnes</div></div>"
  }